<div class="bad-container">
  <h1>friends</h1>
    <% @friendships.each do |friendship| %>
      <%= link_to friendship_path(friendship) do %>
        <p class="link-friend"><%= friendship.user == current_user ? friendship.friend_user.username : friendship.user.username %> </p>
      <% end %>
    <% end %>
</div>
